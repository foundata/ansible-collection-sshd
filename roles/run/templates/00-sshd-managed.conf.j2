# Ansible managed

{% set merged_settings = __run_sshd_sshd_settings_defaults | combine(run_sshd_sshd_settings) %}
{% for key, value in merged_settings.items() %}
{% if value is sameas true %}
{{ key }} yes
{% elif value is sameas false %}
{{ key }} no
{% elif value is iterable and value is not string %}
{% for item in value %}
{{ key }} {{ item }}
{% endfor %}
{% else %}
{{ key }} {{ value }}
{% endif %}
{% endfor %}
