# {{ ansible_managed }}

{%- for key, value in __run_sshd_merged_sshd_settings.items() -%}
  {%- if value is sameas true -%}
    {{- key }} yes
  {%- elif value is sameas false -%}
    {{- key }} no
  {%- elif value is iterable and value is not string -%}
    {%- for item in value -%}
      {{- key }} {{ item }}
    {%- endfor -%}
  {%- else -%}
    {{- key }} {{ value }}
  {%- endif -%}
{%- endfor -%}
